{{ ansible_managed | comment }}
---
global:
  scrape_interval: 15s
  scrape_timeout: 10s

  evaluation_interval: 1m

alerting:
  alertmanager:
    - path_prefix: "{{ compose_base_alertmanager_web_path_prefix }}"
      static_configs:
        - targets:
            - alertmanager:9093

rule_files:
  - /etc/prometheus/rules/*.yaml

scrape_config_files:
  - /etc/prometheus/scrapes/*.yaml

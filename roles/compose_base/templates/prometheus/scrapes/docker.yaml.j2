{{ ansible_managed | comment }}
---
scrape_configs:
  - job_name: docker
    docker_sd_configs:
      - host: unix:///var/run/docker.sock
    relabel_configs:
      - source_labels: [__meta_docker_container_label_prometheus_scrape]
        regex: "true"
        action: keep
  - job_name: node_exporter
    static_configs:
      - targets:
          - host.docker.internal:9090
